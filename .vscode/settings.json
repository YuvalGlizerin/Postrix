{
  "launches": {
    "Service": "Service"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "yaml",
    "github-actions-workflow",
    "json"
  ],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "files.associations": {
    "**/.github/**/*.yml": "github-actions-workflow",
    "**/.github/**/*.yaml": "github-actions-workflow"
  },
  "cSpell.words": [
    "ASPMX",
    "autoincrement",
    "awscli",
    "awsdns",
    "backoffice",
    "bitnami",
    "Capish",
    "Clob",
    "cloudresourcemanager",
    "Creatomate",
    "daemonset",
    "datname",
    "dualstack",
    "emberstack",
    "ethersproject",
    "Googlebot",
    "helmfile",
    "joby",
    "keepalive",
    "kubeconfig",
    "lumo",
    "NEXTAUTH",
    "PGPASSWORD",
    "polymarket",
    "postrix",
    "psql",
    "rabbitmq",
    "sealedbox",
    "toybuttons",
    "yuvadius"
  ],
  "files.exclude": {
    "**/.git": true
  }
}
